{
    "extends": "../.eslintrc.json",
    "rules": {
        "func-names": 0,
        "import/no-extraneous-dependencies": 0,
        "no-param-reassign": 0,
        "no-undef": 0,
        "no-unused-expressions": 0,
        "prefer-arrow-callback": 0,
        "promise/always-return": 0,
				"promise/catch-or-return": 0,
				"arrow-body-style": 0
    }
}
